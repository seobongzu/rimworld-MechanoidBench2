<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	
  <ThingDef ParentName="ResourceBase">
    <defName>VFE_ComponentMechanoid</defName>
    <label>Mechanoid Component</label>
    <description>A high-quality advanced component taken out of the mechanoid. not completely understandable, but it can be used.</description>
    <graphicData>
      <texPath>Things/items/JGHComponent</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>50</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <MarketValue>50</MarketValue>
      <Mass>1</Mass>
      <Flammability>0.6</Flammability>
      <DeteriorationRate>2.0</DeteriorationRate>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
  </ThingDef>

</Defs>